{
  "background": {
    "persistent": false,
    "scripts": [
      "dist/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "assets/icon_64.png",
      "128": "assets/icon_128.png"
    },
    "default_title": "Add Tabs to Reading List"
  },
  "manifest_version": 2,
  "name": "Add Tabs to Reading List",
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "version": "1.0"
}